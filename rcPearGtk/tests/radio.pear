return {
win = Window(),
newStr="Just Clicked button",
label = win.Label(""; 100; 5),
button = win.Button("Click me"; 250; 100),
rbuttonGroup=win.RadioButtonGroup(5; 50; 150),
rbuttonGroup->getElement(0)->setText("radio1"),
