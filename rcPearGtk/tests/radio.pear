rbuttonGroup->getElement(2)->setText("radio3"),
rbuttonGroup->getElement(3)->setText("radio4"),
rbuttonGroup->getElement(4)->setText("radio5"),
button.click(
	label->setText(rbuttonGroup->getSelection()->getText())
